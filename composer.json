{
    "name": "steelwebworks/laravel-shopify",
    "description": "Laravel shopify package",
    "license": "MIT",
    "authors": [
        {
            "name": "Mr Sindhu",
            "email": "sajjad@softwarealliance.dk"
        },
        {
            "name": "Steel Hammer",
            "email": "steelwebworks@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "guzzlehttp/guzzle": ">=6.2",
        "illuminate/support": "^6.0||^7.0||^8.0||^9.0||>=10.0"
    },
    "autoload": {
        "psr-4": {
            "SteelWebWorks\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SteelWebWorks\\Shopify\\ShopifyServiceProvider"
            ],
            "aliases": {
                "Shopify": "SteelWebWorks\\Shopify\\Facades\\Shopify"
            }
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
